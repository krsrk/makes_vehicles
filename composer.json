{
  "name": "minicli/application",
  "description": "Minicli Application Template",
  "license": "MIT",
  "homepage": "https://github.com/minicli/application",
  "keywords": ["cli","command-line", "template"],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Utils/helpers.php",
      "app/config/db.php"
    ]
  },
  "require": {
    "minicli/minicli": "^3.0",
    "minicli/command-help": "^0.1.0",
    "vlucas/phpdotenv": "^v5.5.0",
    "illuminate/database": "^9.46",
    "anourvalar/office": "^2.3",
    "phpoffice/phpspreadsheet": "^1.25",
    "aws/aws-sdk-php": "^3.255",
    "guzzlehttp/guzzle": "^7.0",
    "phpmailer/phpmailer": "^6.7"
  },
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "scripts": {
    "test" : ["pest"]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
